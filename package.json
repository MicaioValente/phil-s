{
  "name": "philsocial",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "i": "yarn && yarn ios-pod",
    "update": "npx expo install --fix && yarn ios-pod",
    "clear": "rm -rf node_modules yarn.lock ios/Pods ios/build ios/Podfile.lock ios/.xcode.env.local",
    "android": "react-native run-android",
    "android-assemble": "cd android && ./gradlew assembleRelease && open ./app/build/outputs/apk/release",
    "android-bundle": "cd android && ./gradlew bundleRelease && open ./app/build/outputs/bundle/release",
    "android-clean": "cd android && ./gradlew clean",
    "android-release": "react-native run-android --mode=release",
    "ios": "react-native run-ios",
    "ios-pod": "cd ios && pod install",
    "ios-release": "react-native run-ios --device \"Name\"",
    "schema-pull": "npx @aws-amplify/cli codegen",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
